	<queryString>
		<![CDATA[SELECT 
	cf.id_certificado_fitosanitario
	, cf.codigo_certificado
	, i.nombre_idioma
	, cf.identificador_exportador
	, tcp.contador_productos
	, LTRIM(COALESCE (STRING_AGG(DISTINCT (COALESCE (ppt.nombre_pais_transito, '')), '; '), '') || '; ' || ppd.nombre_pais_destino, '; ') AS organizacion
	, SUBSTRING (COALESCE(o.razon_social, o.nombre_representante ||' '|| o.apellido_representante) || ', ' || o.direccion, 1, 200) nombre_direccion_exportador
	, SUBSTRING (cf.nombre_consignatario || ', ' || cf.direccion_consignatario, 1, 200) AS nombre_direccion_consignatario
	, cpaor.nombre || ', ' || cpuor.nombre  as lugar_origen
	, SUBSTRING (CASE WHEN i.codigo_idioma = 'SPA' THEN cmt.tipo WHEN i.nombre_idioma = 'Inglés' THEN cmt.tipo_ingles END, 1, 64) AS medio_transporte_declarado
	, SUBSTRING (COALESCE (STRING_AGG(DISTINCT (ppt.nombre_pais_transito || ' - ' || ppt.nombre_puerto_transito), ', ') || '; ', '') || COALESCE (STRING_AGG(DISTINCT (ppd.nombre_pais_destino || ' - ' || ppd.nombre_puerto_destino), ', '), ''), 1, 412) AS punto_entrada_declarado
	, SUBSTRING (COALESCE (cf.nombre_marca, 'N/A'), 1 , 250) AS marca_distintiva
	, SUBSTRING (tcfp.cantidad, 1, 57) || CASE WHEN COUNT(DISTINCT(cfp.id_certificado_fitosanitario_producto)) > 1 THEN CASE WHEN i.codigo_idioma = 'SPA' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END AS numero_descripcion_bultos
	, SUBSTRING (CASE WHEN cf.tipo_solicitud = 'musaceas' THEN 
		tcfp.nombre_producto ||
	  	E'\n' || 'Peso Neto: ' || tcfp.peso_neto || 
	  	E'\n' || 'Peso Bruto: ' || tcfp.peso_bruto || CASE WHEN tcp.contador_productos > 1 THEN CASE WHEN i.codigo_idioma = 'SPA' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END
	  ELSE
		tcfp.nombre_producto ||
		E'\n' || 'Peso Neto: ' || tcfp.peso_neto || CASE WHEN tcp.contador_productos > 1 THEN CASE WHEN i.codigo_idioma = 'SPA' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END
	  END, 1, 167) AS producto_cantidad_declarada
	, tcfp.nombre_botanico AS nombre_botanico
	, 'Por la presente se certifica que las plantas, productos vegetales u otros artículos reglamentados descritos aquí se han inspeccionado y/o sometido a ensayos de acuerdo con los procedimientos oficiales adecuados y se considera que están libres de las plagas cuarentenarias especificadas por la parte contratante importadora y que cumplen los requisitos fitosanitarios vigentes de la parte contratante importadora, incluidos los relativos a las plagas no cuarentenarias reglamentadas.' || E'\nThis is to certify that the plants, plant products or other regulated articles described herein have been inspected and/or tested according to appropiate official procedures and are considered to be free from the quarantine pests specified by the importing contracting party and to conform with the current phytosanitary requirements of the importing contracting party, including those for regulated non-quarantine pests'::text AS texto_nombre_botanico
	, COALESCE (TO_CHAR (cfp.fecha_tratamiento, 'YYYY-MM-DD'), 'N/A') AS fecha_tratamiento
	, COALESCE (SUBSTRING (cfp.producto_quimico, 1, 80), 'N/A') AS producto_quimico
	, COALESCE (SUBSTRING (CASE WHEN i.codigo_idioma = 'SPA' THEN ctt.nombre_tipo_tratamiento ELSE ctt.nombre_tipo_tratamiento_ingles END || ', ' || CASE WHEN i.codigo_idioma = 'SPA' THEN ct.nombre_tratamiento ELSE ct.nombre_tratamiento_ingles END, 1, 80), 'N/A') AS tratamiento
	, COALESCE (SUBSTRING (cfp.duracion || ' ' || CASE WHEN i.codigo_idioma = 'SPA' THEN cdu.nombre_unidad_fitosanitaria ELSE cdu.nombre_unidad_fitosanitaria_ingles END, 1, 50), 'N/A') || ' / ' || COALESCE (SUBSTRING (cfp.temperatura || ' ' || CASE WHEN i.codigo_idioma = 'SPA' THEN cte.nombre_unidad_fitosanitaria ELSE cte.nombre_unidad_fitosanitaria_ingles END, 1, 50), 'N/A') AS duracion_temperatura
	, COALESCE (SUBSTRING (cfp.concentracion || ' ' || CASE WHEN i.codigo_idioma = 'SPA' THEN cco.nombre_unidad_fitosanitaria ELSE cco.nombre_unidad_fitosanitaria_ingles END, 1, 50), 'N/A') AS concentracion
	, SUBSTRING ((COALESCE (cf.informacion_adicional, '')), 1, 761) || CASE WHEN LENGTH (cf.informacion_adicional) > 761 THEN CASE WHEN i.codigo_idioma = 'SPA' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END  || SUBSTRING (CASE WHEN tpro.codigo_tipo_produccion_fitosanitaria = 'ORGA' THEN COALESCE (', Código POA: ' || '(' || string_agg(DISTINCT(cf.codigo_poa), ', ') || ')', '') ELSE '' END, 1, 24) || CASE WHEN cf.codigo_certificado_importacion != '' THEN ', Permiso de importación: ' || cf.codigo_certificado_importacion ELSE '' END || CASE WHEN cf.es_reemplazo = 'Si' THEN '. Este certificado reemplaza y anula al certificado fitosanitario N° ' || tcf.codigo_certificado || ' expedido el ' || TO_CHAR (tcf.fecha_embarque, 'YYYY-MM-DD') ELSE '' END AS informacion_adicional
	, LTRIM(TRIM(STRING_AGG (DISTINCT (trf.nombre_requisito), ', ')), ' ,') AS nombre_requisito
	, LTRIM(TRIM(STRING_AGG (DISTINCT (trf.detalle_requisito), ', ')), ', ') AS detalle_requisito
	, COALESCE (CASE WHEN LENGTH (COALESCE(TRIM(STRING_AGG (DISTINCT (trf.detalle_impreso), ', '), ', '), '') || COALESCE(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', '), '')) <= '480' THEN SUBSTRING ('REQUISITOS FITOSANITARIOS -> ' || COALESCE(TRIM(STRING_AGG (DISTINCT (trf.detalle_impreso), ', '), ', '), '') || CASE WHEN LENGTH(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', ')) > 0 THEN ' / REQUISITOS DE TRÁNSITO -> ' ELSE '' END || COALESCE(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', '), ''), 1, 480) ELSE SUBSTRING ('REQUISITOS FITOSANITARIOS -> ' || COALESCE(TRIM(STRING_AGG (DISTINCT (trf.detalle_impreso), ', '), ', '), '') || CASE WHEN LENGTH(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', ')) > 0 THEN ' / REQUISITOS DE TRÁNSITO -> ' ELSE '' END || COALESCE(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', '), ''), 1, 480) || CASE WHEN LENGTH (COALESCE(TRIM(STRING_AGG (DISTINCT (trf.detalle_impreso), ', '), ', '), '') || COALESCE(TRIM(STRING_AGG (DISTINCT (trt.detalle_impreso), ', '), ', '), '')) > '480' THEN CASE WHEN i.codigo_idioma = 'SPA' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END END , 'N/A') AS detalle_impreso
	, CASE WHEN cf.tipo_solicitud = 'musaceas' OR cf.tipo_solicitud = 'otros' THEN
		CASE WHEN i.codigo_idioma = 'SPA' THEN
			to_char(cf.fecha_embarque, 'TMDay' || ' dd ') || 'de ' || to_char(cf.fecha_embarque, 'TMmonth') || ' de ' || to_char(cf.fecha_embarque, 'yyyy') 
		ELSE
			to_char(cf.fecha_embarque, 'Day' || ' dd ') || TRIM(to_char(cf.fecha_embarque, 'month')) || to_char(cf.fecha_embarque, ' YYYY')
		END
	  ELSE
	  	CASE WHEN i.codigo_idioma = 'SPA' THEN
			to_char(cf.fecha_aprobacion_certificado, 'TMDay' || ' dd ') || 'de ' || to_char(cf.fecha_aprobacion_certificado, 'TMmonth') || ' de ' || to_char(cf.fecha_aprobacion_certificado, 'yyyy') 
		ELSE
			to_char(cf.fecha_aprobacion_certificado, 'Day' || ' dd ') || TRIM(to_char(cf.fecha_aprobacion_certificado, 'month')) || to_char(cf.fecha_aprobacion_certificado, ' YYYY')
		END
	  END AS fecha_aprobacion_certificado_letras
FROM
	g_certificado_fitosanitario.certificado_fitosanitario cf
INNER JOIN g_catalogos.idiomas i ON i.id_idioma = cf.id_idioma
INNER JOIN g_catalogos.tipos_produccion_fitosanitarias tpro ON tpro.id_tipo_produccion_fitosanitaria = cf.id_tipo_produccion
INNER JOIN g_operadores.operadores o ON o.identificador = cf.identificador_exportador
INNER JOIN g_catalogos.localizacion cpaor ON cpaor.id_localizacion = cf.id_pais_origen
INNER JOIN g_catalogos.localizacion cpuor ON cpuor.id_localizacion = cf.id_provincia_puerto_embarque
INNER JOIN g_catalogos.medios_transporte cmt ON cmt.id_medios_transporte = cf.id_medio_transporte
INNER JOIN g_certificado_fitosanitario.paises_puertos_destino ppd ON ppd.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
INNER JOIN g_certificado_fitosanitario.certificado_fitosanitario_productos cfp ON cfp.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
INNER JOIN (SELECT
				cfp.id_certificado_fitosanitario
				, COUNT(DISTINCT(cfp.id_producto)) AS contador_productos
			FROM
				g_certificado_fitosanitario.certificado_fitosanitario_productos cfp
GROUP BY cfp.id_certificado_fitosanitario) tcp ON tcp.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
INNER JOIN (SELECT 
				ep.id_certificado_fitosanitario
				, MIN(ep.id_certificado_fitosanitario_producto) AS id_producto
				, stp.nombre || ' - ' || p.nombre_comun AS nombre_producto
				, p.nombre_cientifico AS nombre_botanico
				, CASE WHEN (SUM (ep.cantidad_comercial)::numeric) % 1 = 0 THEN SUM (ep.cantidad_comercial) ELSE ROUND(CAST(SUM (ep.cantidad_comercial) AS NUMERIC), 2) END || ' ' ||  CASE WHEN i.codigo_idioma = 'SPA' THEN cucc.nombre_unidad_fitosanitaria ELSE cucc.nombre_unidad_fitosanitaria_ingles END AS cantidad 
				, CASE WHEN (SUM (ep.peso_neto)::numeric) % 1 = 0 THEN SUM (ep.peso_neto) ELSE ROUND(CAST(SUM (ep.peso_neto) AS NUMERIC), 2) END || ' ' || CASE WHEN i.codigo_idioma = 'SPA' THEN cupn.nombre_unidad_fitosanitaria ELSE cupn.nombre_unidad_fitosanitaria_ingles END AS peso_neto
				, COALESCE (CASE WHEN (SUM (ep.peso_bruto)::numeric) % 1 = 0 THEN SUM (ep.peso_bruto) ELSE ROUND(CAST(SUM (ep.peso_bruto) AS NUMERIC), 2) END || ' ' || CASE WHEN i.codigo_idioma = 'SPA' THEN cupb.nombre_unidad_fitosanitaria ELSE cupb.nombre_unidad_fitosanitaria_ingles END, 'N/A') AS peso_bruto
			FROM
				g_certificado_fitosanitario.certificado_fitosanitario cf
				INNER JOIN g_catalogos.idiomas i ON i.id_idioma = cf.id_idioma
				INNER JOIN g_certificado_fitosanitario.certificado_fitosanitario_productos ep ON ep.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
				INNER JOIN g_catalogos.productos p ON ep.id_producto = p.id_producto
				INNER JOIN g_catalogos.subtipo_productos stp ON stp.id_subtipo_producto = p.id_subtipo_producto
				INNER JOIN g_catalogos.unidades_fitosanitarias cucc ON cucc.id_unidad_fitosanitaria = ep.id_unidad_cantidad_comercial
				INNER JOIN g_catalogos.unidades_fitosanitarias cupn ON cupn.id_unidad_fitosanitaria = ep.id_unidad_peso_neto
				LEFT JOIN g_catalogos.unidades_fitosanitarias cupb ON cupb.id_unidad_fitosanitaria = ep.id_unidad_peso_bruto
			GROUP BY ep.id_certificado_fitosanitario, stp.nombre, p.nombre_comun, p.nombre_cientifico, i.codigo_idioma, cucc.nombre_unidad_fitosanitaria, cucc.nombre_unidad_fitosanitaria_ingles
			, cupn.nombre_unidad_fitosanitaria, cupn.nombre_unidad_fitosanitaria_ingles, cupb.nombre_unidad_fitosanitaria, cupb.nombre_unidad_fitosanitaria_ingles
			) tcfp ON tcfp.id_producto = cfp.id_certificado_fitosanitario_producto
INNER JOIN (SELECT 
				cf.id_certificado_fitosanitario
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.nombre), ', ')), ', ') AS nombre_requisito
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.detalle), ', ')), ', ') AS detalle_requisito
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.detalle_impreso), ', ')), ', ') AS detalle_impreso
			FROM
				g_certificado_fitosanitario.certificado_fitosanitario cf
				INNER JOIN g_certificado_fitosanitario.paises_puertos_destino ppd ON ppd.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
				INNER JOIN g_certificado_fitosanitario.certificado_fitosanitario_productos cfp ON cfp.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
				INNER JOIN g_requisitos.requisitos_comercializacion rc ON rc.id_producto = cfp.id_producto
				INNER JOIN g_requisitos.requisitos_asignados ra ON rc.id_requisito_comercio = ra.id_requisito_comercio
				INNER JOIN g_requisitos.requisitos r ON ra.requisito = r.id_requisito
			WHERE
				rc.id_localizacion = ppd.id_pais_destino
				AND ra.tipo = 'Exportación'
				AND r.tipo = 'Exportación'
				AND r.estado = 1
				AND ra.estado = 'activo'
				GROUP BY cf.id_certificado_fitosanitario) trf ON trf.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
LEFT JOIN (SELECT 
				id_certificado_fitosanitario
				, codigo_certificado
		   		, fecha_embarque
				FROM
				g_certificado_fitosanitario.certificado_fitosanitario
				) tcf ON tcf.id_certificado_fitosanitario = cf.id_certificado_reemplazo
LEFT JOIN (SELECT 
				cf.id_certificado_fitosanitario
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.nombre), ', ')), ', ') AS nombre_requisito
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.detalle), ', ')), ', ') AS detalle_requisito
				, LTRIM(TRIM(STRING_AGG (DISTINCT (r.detalle_impreso), ', ')), ', ') AS detalle_impreso
			FROM
				g_certificado_fitosanitario.certificado_fitosanitario cf
				INNER JOIN g_certificado_fitosanitario.paises_puertos_transito ppt ON ppt.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
				INNER JOIN g_certificado_fitosanitario.certificado_fitosanitario_productos cfp ON cfp.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
				INNER JOIN g_requisitos.requisitos_comercializacion rc ON rc.id_producto = cfp.id_producto
				INNER JOIN g_requisitos.requisitos_asignados ra ON rc.id_requisito_comercio = ra.id_requisito_comercio
				INNER JOIN g_requisitos.requisitos r ON ra.requisito = r.id_requisito
			WHERE
				rc.id_localizacion = ppt.id_pais_transito
				AND ra.tipo = 'Tránsito'
				AND r.tipo = 'Tránsito'
				AND r.estado = 1
				AND ra.estado = 'activo'
				GROUP BY cf.id_certificado_fitosanitario) trt ON trt.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
LEFT JOIN g_certificado_fitosanitario.paises_puertos_transito ppt ON ppt.id_certificado_fitosanitario = cf.id_certificado_fitosanitario
LEFT JOIN g_catalogos.tipos_tratamiento ctt ON ctt.id_tipo_tratamiento = cfp.id_tipo_tratamiento
LEFT JOIN g_catalogos.tratamientos ct ON ct.id_tratamiento = cfp.id_tratamiento
LEFT JOIN g_catalogos.unidades_fitosanitarias cdu ON cdu.id_unidad_fitosanitaria = cfp.id_duracion
LEFT JOIN g_catalogos.unidades_fitosanitarias cte ON cte.id_unidad_fitosanitaria = cfp.id_temperatura
LEFT JOIN g_catalogos.unidades_fitosanitarias cco ON cco.id_unidad_fitosanitaria = cfp.id_concentracion
WHERE
	cf.id_certificado_fitosanitario = $P{idSolicitud}
GROUP BY cf.id_certificado_fitosanitario, i.nombre_idioma, i.codigo_idioma, o.razon_social, o.nombre_representante, o.apellido_representante, o.direccion, cpaor.nombre, cpuor.nombre, cmt.tipo, cmt.tipo_ingles, cfp.cantidad_comercial, cfp.codigo_unidad_cantidad_comercial
, cfp.peso_neto, cfp.codigo_unidad_peso_neto, cfp.peso_bruto, cfp.codigo_unidad_peso_bruto, cfp.fecha_tratamiento, cfp.producto_quimico, ctt.nombre_tipo_tratamiento, ctt.nombre_tipo_tratamiento_ingles, ct.nombre_tratamiento, cco.nombre_unidad_fitosanitaria_ingles
, ct.nombre_tratamiento_ingles, cfp.duracion, cdu.nombre_unidad_fitosanitaria, cdu.nombre_unidad_fitosanitaria_ingles, cte.nombre_unidad_fitosanitaria, cte.nombre_unidad_fitosanitaria_ingles, cfp.concentracion, cco.nombre_unidad_fitosanitaria, tpro.codigo_tipo_produccion_fitosanitaria, cfp.id_certificado_fitosanitario_producto, ppd.nombre_pais_destino, tcf.codigo_certificado
, tcf.fecha_embarque, tcfp.cantidad, tcfp.nombre_producto, tcfp.peso_neto, tcfp.peso_bruto, tcfp.nombre_botanico, tcp.contador_productos
ORDER BY cfp.id_certificado_fitosanitario_producto ASC
LIMIT 1
]]>
	</queryString>
